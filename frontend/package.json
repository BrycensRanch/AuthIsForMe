		"dotenv-defaults": "^5.0.2",
		"flowbite": "^2.3.0",
		"flowbite-react": "^0.10.0",
		"http-proxy": "^1.18.1",
		"jsvectormap": "^1.5.3",
		"ky": "^1.2.4",
		"localforage": "^1.10.0",
		"match-sorter": "^8.0.0",
		"next-recompose-plugins": "^3.0.0",
		"next-seo": "^6.5.0",
		"next-sitemap": "^4.2.3",
