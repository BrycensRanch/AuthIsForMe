{
	"name": "auth-app-frontend",
	"private": true,
	"version": "1.0.0",
	"description": "Bring your website or plugin to the next level with a simple, yet powerful authentication system. It's free and open source!",
	"main": "index.js",
	"scripts": {
		"dev": "cross-env NODE_ENV=development NODE_OPTIONS='--inspect' next dev",
		"build": "next build",
		"postbuild": "next-sitemap",
		"cleanbuild": "cross-env ANALYZE=true NODE_ENV=production run-s clean build",
		"cleanbuild:start": "run-s clean build start",
		"predocker": "pnpm cleanbuild",
		"docker": "docker build -t chatfrontend . --no-cache",
		"prepm2": "cross-env NODE_ENV=production npm run cleanbuild",
		"pm2": "cross-env NODE_ENV=production pm2 start pm2.config.js",
		"pm2:ts": "cross-env NODE_ENV=production pm2 start --watch --interpreter ts-node-dev pm2.config.ts",
		"start": "next start",
		"build-stats": "cross-env ANALYZE=true npm run build",
		"export": "next export",
		"build-prod": "run-s clean build export",
		"clean": "rimraf .next out",
		"lint": "next lint --fix",
		"lint:check": "next lint",
		"precheck-types": "tsc --noEmit --pretty -p tsconfig-build.json",
		"check-types": "tsc --project cypress --noEmit --pretty -p tsconfig-build.json",
		"test:jest": "cross-env NODE_ENV=test jest",
		"test:cy": "run-s e2e:headless",
		"test": "run-s test:jest test:cy",
		"pretest": "rimraf .nyc_output .next coverage jest-coverage cypress-coverage reports",
		"posttest": "npm run report:combined",
		"mkdir:reports": "shx --silent mkdir reports",
		"precopy:reports": "npm run mkdir:reports",
		"copy:reports": "shx cp cypress-coverage/coverage-final.json reports/from-cypress.json && shx cp jest-coverage/coverage-final.json reports/from-jest.json",
		"precombine:reports": "npm run copy:reports",
		"combine:reports": "pnpm exec nyc merge reports --headed && shx mv coverage.json .nyc_output/out.json",
		"prereport:combined": "npm run combine:reports",
		"report:combined": "pnpm exec nyc report --reporter lcov --reporter text --headed --report-dir coverage",
		"test:watch": "cross-env NODE_ENV=test jest --watch ",
		"healthcheck": "node healthCheck.js",
		"commit": "cz",
		"see-cy-coverage": "pnpm dlx open ./cypress-coverage/lcov-report/index.html",
		"cypress": "cypress open",
		"cypress:headless": "cypress run",
		"dev:cypress": "concurrently --raw \"node testPuppeteer.js\" \"cd .. && npm run dev\"",
		"e2e": "start-server-and-test 'npm run dev:cypress' http://localhost:3000 cypress",
		"e2e:headless": "start-server-and-test 'npm run dev:cypress' http://localhost:3000 cypress:headless",
		"format": "prettier-eslint --log-level info --write \"{,!(node_modules|coverage|dist|out|build|jest-coverage)/**/}*.{js,jsx,ts,tsx}\"",
		"env": "sync-dotenv"
	},
	"nyc": {
		"extends": "./nyc.config.js",
		"report-dir": "cypress-coverage",
		"reporter": [
			"json",
			"lcov",
			"text-summary"
		]
	},
	"keywords": [],
	"dependencies": {
		"@headlessui/react": "^1.7.13",
		"@heroicons/react": "^2.0.16",
		"@sentry/nextjs": "^7.45.0",
		"autoprefixer": "^10.4.14",
		"dotenv-defaults": "^5.0.2",
		"http-proxy": "^1.18.1",
		"next": "^13.2.4",
		"next-recompose-plugins": "^1.0.2",
		"next-seo": "^5.15.0",
		"next-sitemap": "^3.1.55",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"skinview3d": "3.0.0-alpha.1",
		"three": "^0.150.1"
	},
	"devDependencies": {
		"@bahmutov/cypress-code-coverage": "^2.0.1",
		"@commitlint/cli": "^17.5.0",
		"@commitlint/config-conventional": "^17.4.4",
		"@commitlint/cz-commitlint": "^17.5.0",
		"@cypress/browserify-preprocessor": "^3.0.2",
		"@cypress/code-coverage": "^3.10.0",
		"@jest/globals": "^29.5.0",
		"@next/bundle-analyzer": "^13.2.4",
		"@next/env": "^13.2.4",
		"@percy/cli": "^1.21.0",
		"@percy/cypress": "^3.1.2",
		"@testing-library/cypress": "^9.0.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@types/jest": "^29.5.0",
		"@types/node": "^18.15.10",
		"@types/react": "^18.0.29",
		"@types/testing-library__cypress": "^5.0.9",
		"@types/three": "^0.149.0",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"babel-plugin-istanbul": "^6.1.1",
		"commitizen": "^4.3.0",
		"commitlint-config-monorepo": "^2.0.2",
		"concurrently": "^7.6.0",
		"cross-env": "^7.0.3",
		"cypress": "^12.8.1",
		"dotenv-mono": "^1.3.10",
		"eslint": "^8.36.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-next": "^13.2.4",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jest": "^27.2.1",
		"eslint-plugin-jest-dom": "^4.0.3",
		"eslint-plugin-jest-formatting": "^3.1.0",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-sonarjs": "^0.19.0",
		"eslint-plugin-tailwindcss": "^3.10.1",
		"eslint-plugin-testing-library": "^5.10.2",
		"eslint-plugin-unicorn": "^46.0.0",
		"eslint-plugin-unused-imports": "^2.0.0",
		"execa": "^7.1.1",
		"husky": "^8.0.3",
		"ignore-walk": "^6.0.2",
		"jest": "^29.5.0",
		"jest-environment-jsdom": "^29.5.0",
		"jest-fetch-mock": "^3.0.3",
		"jest-mock-fetch": "^2.0.5",
		"lint-staged": "^13.2.0",
		"node-mocks-http": "^1.12.2",
		"npm-run-all": "^4.1.5",
		"nyc": "^15.1.0",
		"postcss": "^8.4.21",
		"prettier": "^2.8.7",
		"prettier-eslint-cli": "^7.1.0",
		"rimraf": "^4.4.1",
		"shx": "^0.3.4",
		"start-server-and-test": "^2.0.0",
		"sync-dotenv": "^2.7.0",
		"tailwindcss": "^3.2.7",
		"typescript": "^5.0.2"
	},
	"author": "",
	"config": {
		"commitizen": {
			"path": "@commitlint/cz-commitlint"
		}
	},
	"license": "MIT",
	"packageManager": "pnpm@7.30.3",
	"engines": {
		"node": ">=16",
		"pnpm": ">=7",
		"npm": ">=8.5.0"
	}
}
