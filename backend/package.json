{
  "name": "auth-app-backend",
  "version": "1.0.0",
  "private": true,
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "test": "cross-env NODE_ENV=test NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest",
    "test:watch": "cross-env NODE_ENV=test NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest --watch",
    "start": "cross-env NODE_ENV=production node dist/app.js",
    "prepm2": "pnpm cleanbuild",
    "pm2": "cross-env NODE_ENV=production pm2 start --watch --interpreter ts-node-dev pm2.config.js",
    "healthcheck": "node healthCheck.js",
    "build": "tsc",
    "build:ts": "tsc -p tsconfig.json",
    "cleanbuild": "run-s clean build:ts",
    "check-types": "tsc --noEmit --pretty",
    "predocker": "pnpm cleanbuild",
    "docker": "docker build -t chatbackend . --no-cache --env-file ../.env",
    "clean": "rimraf dist",
    "dev": "ts-node-dev --respawn --transpile-only -r fix-esm src/app.ts",
    "format": "prettier-eslint --write \"{,!(node_modules)/**/}*.{js,jsx,ts,tsx}\"",
    "commit": "cz",
    "migrate:postgres": "dotenv -e ../.env -- npx prisma migrate deploy"
  },
  "dependencies": {
    "@fastify-decorators/simple-di": "4.0.0-next.4",
    "@fastify/autoload": "^5.7.1",
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^10.1.0",
    "@fastify/secure-session": "^6.0.0",
    "@fastify/sensible": "^5.2.0",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.4.0",
    "@immobiliarelabs/fastify-sentry": "^5.0.2",
    "@mgcrea/fastify-graceful-exit": "^0.4.0",
    "@mgcrea/fastify-session": "^1.1.0",
    "@mgcrea/fastify-session-redis-store": "^1.0.0",
    "@prisma/client": "^4.10.1",
    "dotenv-defaults": "^5.0.2",
    "dotenv-mono": "^1.3.9",
    "fastify": "^4.12.0",
    "fastify-allow": "^1.2.2",
    "fastify-cli": "^5.7.1",
    "fastify-decorators": "^3.15.0",
    "fastify-get-head": "^1.1.0",
    "fastify-grant": "^5.4.8",
    "fastify-impressions": "^1.0.5",
    "fastify-ip": "^0.1.1",
    "fastify-plugin": "^4.5.0",
    "fastify-print-routes": "^2.0.7",
    "fastify-request-timing": "^2.0.2",
    "fastify-user-agent": "^1.0.1",
    "inclusion": "github:matherm-aboehm/inclusion#fix-1",
    "ioredis": "^5.3.0",
    "prisma-redis-middleware": "^4.6.1",
    "prisma-zod-generator": "^0.8.11",
    "scan-env": "github:BrycensRanch/scan-env",
    "typescript": "^4.9.5",
    "useragent": "^2.3.0",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.3",
    "@types/dotenv-defaults": "^2.0.1",
    "@types/ioredis-mock": "^8.2.1",
    "@types/jest": "^29.4.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.19",
    "@types/useragent": "^2.3.1",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fix-esm": "^1.0.1",
    "ioredis-mock": "^8.2.6",
    "jest": "^29.4.3",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "mocha": "^10.2.0",
    "node-dev": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "prettier-eslint-cli": "^7.1.0",
    "prisma": "^4.10.1",
    "prisma-json-schema-generator": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  },
  "prisma": {
    "seed": "ts-node-esm ./prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT"
}
